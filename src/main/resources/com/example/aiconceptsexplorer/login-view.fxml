<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aiconceptsexplorer.controllers.loginController">
    <children>
        <Label alignment="CENTER" text="Log In">
            <font>
                <Font size="26.0" />
            </font>
        </Label>
        <TextField fx:id="email" promptText="Email" />
        <PasswordField fx:id="password" promptText="Password" />
        <Button mnemonicParsing="false" onAction="#onLogin" text="Log In" />
        <Label fx:id="errorLabel" textFill="RED" />
        <Label text="Don't have an account?" />
        <Button fx:id="signupButton" mnemonicParsing="false" onAction="#onSignupRedirect" text="Sign Up" />
    </children>
</VBox>
